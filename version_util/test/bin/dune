(executables
 (modes byte exe)
 (names main)
 (libraries version_util)
 (modules main)
 (preprocess
  (pps ppx_jane)))

(executables
 (modes byte exe)
 (names test_rewrite)
 (libraries core core_unix core_unix.filename_unix core_unix.sys_unix
   version_util)
 (modules test_rewrite)
 (preprocess
  (pps ppx_jane)))

(rule
 (alias default)
 (deps main.exe)
 (action
  (bash ./main.exe)))

(rule
 (alias javascript-runtest)
 (deps main.bc.js)
 (action
  (bash "node main.bc.js")))

(rule
 (alias default)
 (deps test_rewrite.exe main.exe)
 (action
  (bash ./test_rewrite.exe)))
